{
  "name": "slack-poll",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "prestart": "npm run migrate",
    "dev": "nodemon index.js",
    "test": "jest --config jest.json",
    "migrate": "if [ $DATABASE_URL ]; then sequelize db:migrate --url $DATABASE_URL --migrations-path app/infrastructure/sequelize/migrations ; else echo; fi",
    "lint": "eslint app/**/*.js --fix",
    "format": "prettier app/**/*.js --write"
  },
  "engines": {
    "node": "12.x"
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@slack/interactive-messages": "^1.4.0",
    "@slack/web-api": "^5.6.0",
    "compression": "^1.7.4",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "helmet": "^3.21.2",
    "morgan": "^1.9.1",
    "pg": "^7.17.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "husky": "^3.1.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.4.3",
    "nodemon": "^2.0.2",
    "prettier": "^1.16.4",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2"
  }
}
