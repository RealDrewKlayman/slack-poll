{
  "name": "slack-poll",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "prestart": "npm run migrate",
    "dev": "nodemon index.js",
    "test": "jest --config jest.json",
    "migrate": "if [ $DATABASE_URL ]; then sequelize db:migrate --url $DATABASE_URL ; else echo; fi",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier *.js --write"
  },
  "engines": {
    "node": "12.x"
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.0"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^3.1.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.4.3",
    "moxios": "^0.4.0",
    "nodemon": "^1.18.7",
    "prettier": "^1.16.4",
    "sequelize-mock": "^0.10.2",
    "supertest": "^4.0.2"
  }
}
