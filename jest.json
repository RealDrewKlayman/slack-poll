{
  "testPathIgnorePatterns": ["/node_modules/", "/test/"],
  "setupFiles": ["./test/config.js"],
  "collectCoverage": true,
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/test/"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  }
}
